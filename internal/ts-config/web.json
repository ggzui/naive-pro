{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Web Config",
  "extends": "./base.json",
  "compilerOptions": {
    "useDefineForClassFields": true,
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "noImplicitAny": false,
    "moduleResolution": "bundler",
    "types": ["@naive-pro/types/global"]
  }
}
